    <parameter name="environment" value="local"/>
    <test name="Search" verbose="2" preserve-order="true" enabled="true">
        <groups>
            <run>
                <include name = "SearchTests"></include>
            </run>
        </groups>
        <classes>
            <class name="ExampleTests.TestNG_ExampleTests">
                <methods>
                    <include name="test_GoogleSearch"/>
                </methods>
            </class>
        </classes>
    </test>

